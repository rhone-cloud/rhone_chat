{
  "module": "rhone_chat",
  "routes": {
    "dir": "app/routes",
    "output": "app/routes/routes_gen.go",
    "package": "routes"
  },
  "static": {
    "dir": "public",
    "prefix": "/"
  },
  "dev": {
    "port": 8000,
    "hotReload": true,
    "ignore": [
      "db/*.sqlite",
      "db/*.sqlite-wal",
      "db/*.sqlite-shm"
    ]
  },
  "build": {
    "output": "dist"
  },
  "devtools": {
    "enabled": true
  },
  "tailwind": {
    "enabled": true,
    "input": "app/styles/input.css",
    "output": "public/styles.css"
  },
  "state": {
    "enabled": true,
    "manifest": "vango_state_manifest.json",
    "schema": "vango_state_schema.json",
    "bindings_glob": "**/vango_state_bindings_gen.go",
    "auto_apply_safe": true
  },
  "strict": "default"
}
